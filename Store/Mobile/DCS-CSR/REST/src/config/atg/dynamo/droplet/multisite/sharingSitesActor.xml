<actor-template>
  <!-- Add the site icon to the output -->  
  <actor-chain id="mobileSites">
    <droplet id="sharingSites">
      <oparam name="output">
        <droplet id="forEach">
          <oparam name="output">
            <output id="id" name="${forEach.element.id}.siteIcon" value="${forEach.element.largeSiteIcon}"/>
          </oparam>
        </droplet>
      </oparam>
    </droplet>
  </actor-chain>
</actor-template>
